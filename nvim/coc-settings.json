{
  "coc.preferences.formatOnSaveFiletypes": [
    "css",
    "Markdown",
    "javascript",
    "go",
    "json",
    "cs",
    "dart",
    "rust"

  ],
  "languageserver": {
    "ccls": {
      "command": "ccls",
      "filetypes": ["c", "cpp", "objc", "objcpp"],
      "rootPatterns": [
        ".ccls",
        "compile_commands.json",
        ".vim/",
        ".git/",
        ".hg/"
      ],
      "initializationOptions": {
        "cache": {
          "directory": "/tmp/ccls"
        }
      }
    }
  },
}
